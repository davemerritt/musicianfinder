<% if @reference.errors.present? %>
  $("#reference-form-errors").html("<%= escape_javascript(render(:partial => '/shared/error_messages')) %>");
<% else %>
  $("#feedback-rating").html("<%= escape_javascript(render(:partial => '/users/feedback_rating')) %>");
  $("#references").html("<%= escape_javascript(render(:partial => '/users/references')) %>");
  $("#new_reference")[0].reset();
<% end %>