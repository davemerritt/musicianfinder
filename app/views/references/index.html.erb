<div class="row">  
  <% @my_refs.each do |ref| %>

    <h4>Reference Left For: <%= ref.target_id %></h4>
    <p>
      Feedback: <%= ref.feedback_textifier %><br>
      Content: <%= ref.comment %><br>
      Jammed: <%= ref.check("jammed") %>
      Performed: <%= ref.check("performed") %>
      Recorded: <%= ref.check("recorded") %><br>
      Last Updated: <%= ref.updated_at %><br>
      <%= link_to "Update This Reference","references/#{ref.id}" %><br>
    </p>
    <hr />
  <% end %>
</div>
  