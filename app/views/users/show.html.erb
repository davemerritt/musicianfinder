<ul>
	<li>Feedback_rating: <%= @user.target_references.sum(:feedback_value) %></li>
	<li>Feedback_percent_positive: <%= number_to_percentage(100*
	(@user.target_references.sum(:feedback_value) / @user.target_references.count(:feedback_value).to_f), precision: 1 ) %></li>
  <li>first_name: <%= @user.first_name %></li>
  <li>last_name: <%= @user.last_name %></li>
  <li>Genres: <%= @user.genre_list %></li>
  <li>Instruments: <%= @user.instrument_list %></li>
  <li>Email: <%= @user.email %></li>
  <li>city: <%= @user.city %></li>
  <li>postal_code: <%= @user.postal_code %></li>
  <li>bio: <%= @user.bio %></li>
</ul>


	<div class="Reference form">
	<%= render "references" %>
