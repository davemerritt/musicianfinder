<% if @user.target_references.count >= 1 %>
<h4>References left for <%= @user.first_name.capitalize %></h4>
<% end %>
<% @user.target_references.each do |reference| %>
    <% unless reference.new_record? %>
      Feedback_value: <%= reference.feedback_textifier %><br> 
      Comment: <%= reference.comment %><br>
      Jammed together: <%= reference.jammed %><br>
      Performed: <%= reference.performed %><br>
      Recorded: <%= reference.recorded %>
      
        <% if reference.source != nil %>
        <em>by <%= reference.source.full_name %></em>
         Reference Created on: <%= reference.created_at %><hr>
        <% end %>
    <% end %>
  <% end %>